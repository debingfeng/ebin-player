{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "declaration": true,
    "declarationDir": "dist",
    "emitDeclarationOnly": false,
    "jsx": "react-jsx",
    "module": "CommonJS",
    "target": "ES2019",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "isolatedModules": true,
    "noEmit": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": false
  },
  "include": ["src"]
}


