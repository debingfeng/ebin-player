{
  "name": "@ebin-player/react",
  "version": "0.0.4",
  "description": "React bindings for ebin-player",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "dependencies": {
    "@ebin-player": "^0.0.4"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.0.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --ci --watchAll=false",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests",
    "prepublishOnly": "pnpm run build && pnpm run test:ci"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/debingfeng/ebin-player.git"
  },
  "bugs": {
    "url": "https://github.com/debingfeng/ebin-player/issues"
  },
  "homepage": "https://github.com/debingfeng/ebin-player#readme"
}
