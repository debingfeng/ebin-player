{
  "name": "@ebin-player/vue3",
  "version": "0.0.4",
  "description": "Vue 3 bindings for ebin-player",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@ebin-player/core": "^0.0.4"
  },
  "devDependencies": {
    "@types/node": "^24.6.2",
    "@vue/test-utils": "^2.4.0",
    "@vue/vue3-jest": "^29.2.6",
    "babel-jest": "^29.7.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.0.0",
    "vue": "^3.3.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --ci --watchAll=false",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests",
    "prepublishOnly": "pnpm run build"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/debingfeng/ebin-player.git"
  },
  "bugs": {
    "url": "https://github.com/debingfeng/ebin-player/issues"
  },
  "homepage": "https://github.com/debingfeng/ebin-player#readme",
  "publishConfig": {
    "access": "public"
  }
}
